<Window x:Class="GrandOS.Windows.AddProgramPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GrandOS.Windows"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Topmost="True"
        Deactivated="window_Deactivated"
        ResizeMode="NoResize"
        WindowStyle="None"
        Title="AddProgramPopup" Height="1000" Width="600">
    <Viewbox StretchDirection="UpOnly" Stretch="UniformToFill">
        <Grid x:Name="titleGrid" Background="{DynamicResource BackgroundDark}" Height="1000" Width="600">
            

        </Grid>
    </Viewbox>
</Window>
