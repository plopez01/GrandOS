<Window x:Name="window" x:Class="GrandOS.Windows.AlwaysOnTop"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GrandOS.Windows"
        mc:Ignorable="d"
		WindowStyle="None"
		Topmost="True"
		AllowsTransparency="True"
		Deactivated="window_Deactivated"
		ShowInTaskbar="False"
        Title="AlwaysOnTop" Height="100" Width="100" Background="Transparent">
	<Border BorderBrush="#FF23366A" BorderThickness="4" CornerRadius="90" Name = "MainBorder" Background="#FF111A29" MouseDown="MainBorder_MouseDown" MouseEnter="MainBorder_MouseEnter">
		
	</Border>

</Window>
